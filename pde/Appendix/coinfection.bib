	Keywords = {body condition, bovine tuberculosis, dairy-cows, endoparasites, epidemiology, infection, kruger-national-park, Kruger National Park, milk-production, Mycobacterium bovis, mycobacterium-bovis, nutritional-status, population, snowshoe hares, South Africa, Syncerus caffer, wildlife disease},
