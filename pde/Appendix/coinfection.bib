	File = {Mathematical%20models%20of%20malaria:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/DB5ZZVAV/Mathematical models of malaria.pdf:application/pdf},
	Issn = {1475-2875},
	Journal = {Malar J},
	Keywords = {Disease Information, host-parasite interactions, humans, Incidence, malaria, Models, Theoretical, research support, non-u.s. gov't, review},
	Language = {eng},
