	Issn = {1865-1674},
	Journal = {Transboundary and Emerging Diseases},
	Keywords = {ATTACK TOLERANCE, cattle, CENTRALITY, complex networks, epidemics, France, great-britain, livestock, movements, network analysis, patterns, SCALE-FREE NETWORKS, Trade, transmission},
	Language = {English},
