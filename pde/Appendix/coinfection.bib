	Keywords = {comparative study, Computational Biology, Computer Simulation, Models, Statistical, Numerical Analysis, Computer-Assisted, research support, n.i.h., extramural, research support, non-u.s. gov't, review, sensitivity, sensitivity and specificity, Systems Biology, Technical Papers, uncertainty},
