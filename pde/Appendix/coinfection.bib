	File = {The%20relative%20contribution%20of%20c:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/JBDG65ZU/The relative contribution of c.pdf:application/pdf},
	Issn = {1471-2954},
	Journal = {Proc Biol Sci},
	Keywords = {Adolescent, Ancylostomatoidea, Animals, Ascaris lumbricoides, Child, Child, Preschool, co-infection, coinfection, Disease Ecology \& Co-infection, feces, Female, Fever, Focal Infection, Helminthiasis, humans, Linear Models, Male, Parasite Egg Count, research support, non-u.s. gov't, Risk Factors, Socioeconomic Factors, tanzania, Trichuris},
