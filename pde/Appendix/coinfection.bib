	Issn = {0022-5193},
	Journal = {Journal of Theoretical Biology},
	Month = sep,
	Note = {WOS:000339126300016},
	Pages = {174--191},
	Title = {Comparing dengue and chikungunya emergence and endemic transmission in {A}. aegypti and {A}. albopictus},
	Volume = {356},
	Year = {2014},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.jtbi.2014.04.033}}

