	File = {The%20Effect%20of%20Temperature%20on%20L:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/WC6UJJB5/The Effect of Temperature on L.pdf:application/pdf},
