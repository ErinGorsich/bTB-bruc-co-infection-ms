	Issn = {0090-3558},
	Journal = {Journal of Wildlife Diseases},
	Keywords = {antibody-responses, antigens, bovine tuberculosis, calmette-guerin, diagnosis, diagnostic tests, Mycobacterium bovis, Odocoileus, tuberculosis, usa, vaccination, virginianus, white-tailed deer},
	Language = {English},
	Month = jan,
	Note = {WOS:000263391300015},
