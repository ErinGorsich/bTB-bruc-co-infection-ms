@article{georgiadis_correlation-adjusted_2003,
	Abstract = {Models for multiple-test screening data generally require the assumption that the tests are independent conditional on disease state. This assumption may be unreasonable, especially when the biologic basis of the tests is the same. We propose a model that allows for correlation between two diagnostic test results. Since models that incorporate test correlation involve more parameters than can be estimated with the available data, posterior inferences will depend more heavily on prior distributions, even with large sample sizes. If we have reasonably accurate information about one of the two screening tests (perhaps the standard currently-used test) or the prevalences of the tested populations, accurate inferences about all the parameters, including the test correlation, are possible. In this paper, we present a model for evaluating dependent diagnostic tests and analyze real and simulated data sets. Our analysis shows that when the tests are correlated, a model that assumes conditional independence can perform very poorly. We recommend that, if the tests are only moderately accurate and measure the same biological responses, researchers use the dependence model for their analyses.},
	Author = {Georgiadis, M.P. and Johnson, W.O. and Ramanpreet, S. and Gardner, I.A.},
	Journal = {Journal of Applied Statistics},
	Pages = {63--76},
	Title = {Correlation-adjusted estimation of sensitivity and specificity of two diagnostic tests},
	Volume = {52},
	Year = {2003}}

@article{godfroid_how_2002,
	Abstract = {Collaborative work was {\textregistered}nanced by the EU to develop and assess new diagnostic tools that can differentiate between bovine brucellosis and bovine infections due to Yersinia enterocolitica O:9 either in conjunction with, or as an alternative to, the classical serological, bacteriological or allergic skin tests. Sixteen heifers were experimentally infected with Brucella abortus biovar 1 ({\textregistered}ve heifers), Brucella suis biovar 2 (two heifers), Y. enterocolitica O:9 (six heifers) and Y. enterocolitica O:3 (three heifers). Four heifers, naturally infected with Y. enterocolitica O:9 that presented aspeci{\textregistered}c brucellosis serological reactions were also included in the experiment. A self-limited infection was induced in cattle by B. suis biovar 2. All the brucellosis serological tests used, i.e. the slow agglutination test (SAW), the Rose Bengal test (RB), the complement {\textregistered}xation test (CFT), indirect and competitive ELISA's, lacked speci{\textregistered}city when used to analyze sera from Y. enterocolitica O:9 infected animals. A Yersinia outer membrane proteins (YOPs)-ELISA was also used and although the test is able to detect a Yersinia group infection, it provided no evidence of whether or not there is a possible brucellosis infection when dual infections are present. The brucellergen IFN-g test showed a lack of speci{\textregistered}city also. The only test that was proven to be speci{\textregistered}c is the brucellergen skin test. All brucellosis serological tests, except the indirect ELISA, were limited in their ability to detect B. abortus persistently infected animals.
Based on these experimental studies, a strategy was implemented as part of the year 2001 Belgian Brucellosis Eradication Program to substantiate the eradication of bovine brucellosis. Epidemiolo- gical inquiries have identi{\textregistered}ed risk factors associated with aspeci{\textregistered}c serological reactions, possible transmission and infection of cattle by B. suis biovar 2 from infected wild boars; and both legal and administrative measures taken by the veterinary services. No cases of bovine brucellosis have been con{\textregistered}rmed in Belgium since March 2000.},
	Author = {Godfroid, J and Saegerman, C. and Wellemans, V. and Walravens, K. and Letesson, J. and Tibor, A. and McMillan, A. and Spencer, S. and Sanna, M. and Bakker, D. and Pouillot, R. and Garin-Bastuji, B.},
	Journal = {Veterinary Microbiology},
	Pages = {461--477},
	Title = {How to substantiate eradication of bovine brucellosis when aspecific serological reactions occur in the course of brucellosis testing},
	Volume = {90},
	Year = {2002}}

