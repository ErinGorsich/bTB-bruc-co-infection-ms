	Journal = {Veterinary Microbiology},
	Keywords = {Antibody response, Carpodacus mexicanus, chickens, Conjunctivitis, diversity, experimental-infection, expression, gene, House finch, Mycoplasma gallisepticum, pulmonis, transmission, vivo, wildlife disease},
	Language = {English},
