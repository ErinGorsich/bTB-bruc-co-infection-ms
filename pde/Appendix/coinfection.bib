	Keywords = {body weight, co-infection, concomitant infection, concurrent infection, Eimeria spp., gastrointestinal nematodes, lambs, Melophagus ovinus, ostertagia-circumcincta, ovis-aries l., resistance, soay sheep, St Kilda, Teladorsagia spp., trichostrongylus-colubriformis, Trichostrongylus spp., ungulate population, wood mice},
	Language = {English},
