	Month = may,
	Number = {5},
	Pages = {872--874},
	Title = {Role of {Transportation} in {Spread} of {Porcine} {Epidemic} {Diarrhea} {Virus} {Infection}, {United} {States}},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4012813/},
	Volume = {20},
