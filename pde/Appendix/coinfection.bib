	Keywords = {aids, coinfection, community, Concomitant disease, Host health, impact, infections, Integrated control, neglected tropical diseases, parasite, Parasite infracommunity, Pathogen abundance, Pathogen diversity, Pathogen-host interactions, Polymicrobial infection, reality, virus, Within-host parasite ecology},
