	Note = {WOS:000302787600005},
	Number = {3},
	Pages = {255--264},
	Title = {Innate {Immunity} in {Free}-{Ranging} {African} {Buffalo} ({Syncerus} caffer): {Associations} with {Parasite} {Infection} and {White} {Blood} {Cell} {Counts}},
