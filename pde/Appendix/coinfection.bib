	Issn = {2213-2244},
	Journal = {International Journal for Parasitology: Parasites and Wildlife},
	Keywords = {Animal behaviour, conservation, ecology, Male-bias parasite burdens, Parasite transmission, Sociality, Social network},
	Month = dec,
	Pages = {235--245},
	Title = {Networks and the ecology of parasite transmission: {A} framework for wildlife parasitology},
