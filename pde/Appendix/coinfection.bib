	Pages = {e1002345},
	Title = {Network {Model} of {Immune} {Responses} {Reveals} {Key} {Effectors} to {Single} and {Co}-infection {Dynamics} by a {Respiratory} {Bacterium} and a {Gastrointestinal} {Helminth}},
