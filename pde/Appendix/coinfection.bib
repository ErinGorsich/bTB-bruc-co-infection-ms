	Keywords = {Animal Husbandry, Animals, cattle, Cattle Diseases, Disease Outbreaks, Domestic, FMDV, models, network, non-u.s. gov't, research support, Swine, Theoretical, United Kingdom},
	Number = {15},
	Pages = {669--74},
	Pmcid = {PMC2373390},
	Title = {Recent network evolution increases the potential for large epidemics in the {British} cattle population.},
	Volume = {4},
	Year = {2007},
