	Keywords = {Animals, Beetles, Crustacea, Culicidae, Emperical, Food Web \& Community Structure Literature, Hemiptera, Insect Literature, Rain, research support, non-u.s. gov't, Salinity, Western Australia, Wetlands},
	Language = {eng},
