@article{waruiru_prevalence_2000,
	Abstract = {A survey of gastrointestinal parasite infections of young (56 months old), immature (6{\textasciicircum}12 months old) and adult (412 months old) dairy cattle on 16 farms in Kiambu District, Kenya was conducted during a dry season (September 1991 to January 1992) and during a wet season (March to July 1992). The survey was based on monthly coproparasitological examination of cohorts and worm counts in tracer calves. The e{!'}ects of age, sex, farm and season on the prevalence and intensity of helminth and coccidial infections were determined. Faecal egg and oocyst counts revealed that the overall prevalences were: strongyles (including trichostrongyles) (85.5\%), liver ukes (Fasciola gigantica) (34.0\%), coccidia (30.9\%) and tapeworms (9.6\%). Eight species of the protozoan Eimeria were identied, the most prevalent species being E. bovis and E. zuernii. The most prevalent nematode genera were Haemonchus, Cooperia, Oesophagostomum and Trichostrongylus. Season, farm and age of the animals had a signicant (p50.05) inuence on the intensity of infection with strongyles, liver ukes and coccidia, whereas the sex of the animals had no signicant (p40.05) e{!'}ect on the prevalence or intensity of infections. A higher intensity of infection with strongyles and coccidia was found in the wet season than in the dry season (p50.05). The age-specic intensity was in the following order: for strongyles, immature animals of 6{\textasciicircum}12 months of age had the highest egg counts, followed by young calves and adults. Calves had signicantly (p50.05) higher oocyst counts than immatures or adults. Liver uke egg counts did not di{!'}er signicant (p40.05) between immatures and adult cattle.},
	Author = {Waruiru, R.M. and Kyvsgaard, N.C. and Thamsborg, S.M. and Nansen, P. and Bogh, H.O. and Munyua, W.K. and Gathuma, J.M.},
	Journal = {Veterinary Research Communications},
	Pages = {39--53},
	Title = {The prevalence and intensity of helminth and coccidial infections in dairy cattle in central {Kenya}},
	Volume = {24},
	Year = {2000}}

