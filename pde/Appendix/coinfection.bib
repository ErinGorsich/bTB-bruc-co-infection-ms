@article{jones_identifiability_2010,
	Abstract = {We discuss the issue of identifiability of models for multiple dichotomous diagnostic tests in the absence of a gold standard (GS) test. Data arise as multinomial or product-multinomial counts depending upon the number of populations sampled. Models are generally posited in terms of population prevalences, test sensitivities and specificities, and test dependence terms. It is commonly believed that if the degrees of freedom in the data meet or exceed the number of parameters in a fitted model then the model is identifiable. Goodman (1974, Biometrika 61, 215-231) established that this was not the case a long time ago. We discuss currently available models for multiple tests and argue in favor of an extension of a model that was developed by Dendukuri and Joseph (2001, Biometrics 57, 158-167). Subsequently, we further develop Goodman's technique, and make geometric arguments to give further insight into the nature of models that lack identifiability. We present illustrations using simulated and real data.},
	Author = {Jones, Geoffrey and Johnson, Wesley O. and Hanson, Timothy E. and Christensen, Ronald},
	Doi = {10.1111/j.1541-0420.2009.01330.x},
	Issn = {0006-341X},
	Journal = {Biometrics},
	Keywords = {conditional dependence, Correlated tests, error, Latent class model, latent class models, prevalence, sensitivity, specificity},
	Language = {English},
	Note = {WOS:000281950000020},
	Number = {3},
	Pages = {855--863},
	Title = {Identifiability of {Models} for {Multiple} {Diagnostic} {Testing} in the {Absence} of a {Gold} {Standard}},
	Volume = {66},
	Year = {2010},
	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1541-0420.2009.01330.x}}

