	Title = {Bayesian sample size for diagnostic test studies in the absence of a gold standard: {Comparing} identifiable with non-identifiable models},
	Volume = {29},
	Year = {2010},
	Bdsk-Url-1 = {http://dx.doi.org/10.1002/sim.4037}}

