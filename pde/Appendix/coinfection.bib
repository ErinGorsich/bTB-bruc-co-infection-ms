	Issn = {1553-7374},
	Journal = {PLoS Pathog},
	Keywords = {Animals, biography, Communicable Disease Control, Culicidae, Disease Transmission, Infectious, historical article, History, 20th Century, humans, malaria, Models, Biological, research support, n.i.h., extramural, research support, non-u.s. gov't, research support, u.s. gov't, p.h.s.},
