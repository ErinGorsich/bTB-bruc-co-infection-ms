	Keywords = {bovine tuberculosis, Cattle movements, CENTRALITY, dairy-cattle, great-britain, INTERVENTIONS, MOUTH-DISEASE EPIDEMIC, mycobacterium-bovis, network analysis, spread, surveillance, trade patterns, VETERINARY-MEDICINE},
