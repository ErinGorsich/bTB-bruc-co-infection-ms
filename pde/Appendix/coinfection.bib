	Issn = {0030-2465},
	Journal = {Onderstepoort Journal of Veterinary Research},
	Keywords = {African buffalo, bovine tuberculosis, cattle, diagnosis, foot-and-mouth disease, gamma, linked-immunosorbent-assay, mouth-disease, Syncerus caffer, virus, wildlife},
	Language = {English},
	Month = jun,
	Note = {WOS:000231943800005},
