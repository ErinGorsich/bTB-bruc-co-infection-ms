	Issn = {1461-0248 1461-023X},
	Journal = {Ecology letters},
	Keywords = {*Animals, Wild, *Models, Theoretical, Animal Diseases/epidemiology, Animals, Chiroptera/virology, ecology, Epidemiologic Studies, Infection/*epidemiology, Lyssavirus, Plague/transmission/veterinary, Rhabdoviridae Infections/transmission/veterinary, Sciuridae/virology},
	Language = {eng},
	Month = oct,
	Number = {10},
	Pages = {1083--1094},
