	Issn = {0066-4170},
	Journal = {Annu Rev Entomol},
	Keywords = {Animals, Competitive Behavior, Culicidae, Cyprinodontiformes, ecosystem, Emperical, Food Web \& Community Structure Literature, Insect Literature, Larva, Oryza, Oviposition, population dynamics, Predatory Behavior, research support, non-u.s. gov't, Symbiosis},
