	Journal = {Journal of the Royal Society Interface},
	Keywords = {African lion, african lions, behavior, bond percolation, contact rates, cooperation, dispersal, dynamics, epidemic, epidemiological modelling, infectious-disease, network model, patterns, social-organization, territoriality, wildlife disease},
	Language = {English},
	Month = jun,
	Note = {WOS:000289672200002},
