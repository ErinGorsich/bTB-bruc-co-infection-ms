	File = {A%20research%20agenda%20for%20malaria:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/2WGVRR3H/A research agenda for malaria.PDF:application/pdf},
	Issn = {1549-1676},
	Journal = {PLoS Med},
	Keywords = {Animals, Anopheles, Carrier State, Databases, Factual, Disease \& Mozy Models, Drug resistance, Feasibility Studies, Global Health, Health Policy, humans, Insecticide Resistance, Insect Vectors, Internet, malaria, Malaria model, Models, Theoretical, Mosquito Control, Planning Techniques, Plasmodium, Research, research support, non-u.s. gov't, Resource Allocation, review, Socioeconomic Factors, User-Computer Interface},
