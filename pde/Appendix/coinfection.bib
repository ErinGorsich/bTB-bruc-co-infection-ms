	Journal = {Preventive Veterinary Medicine},
	Keywords = {abortus strain rb51, animal brucellosis, antibiotic-therapy, Bioterrorism, Brucella, brucellosis, epidemiology, Immunity, intracellular life, marine mammal brucella, melitensis infection, Metabolism, "One Health", polymerase-chain-reaction, real-time pcr, risk-factors, tandem-repeat analysis, Taxonomy, Vaccine, Virulence, Zoonosis},
	Language = {English},
