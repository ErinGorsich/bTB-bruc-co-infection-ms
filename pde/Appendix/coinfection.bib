	Keywords = {avium subsp-paratuberculosis, Bayesian estimation, bovine paratuberculosis, complement-fixation test, disease prevalence, elisa, faecal cultural, gold standard, johnes-disease, latent class models, linked-immunosorbent-assay, mycobacterium-paratuberculosis, ovine paratuberculosis, paratuberculosis, restriction-endonuclease analysis, ROC analysis},
	Language = {English},
