	Doi = {10.1111/tbed.12437},
	File = {Data-Driven%20Models%20of%20Foot-and:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/4FNR44WU/Data-Driven Models of Foot-and.pdf:application/pdf},
