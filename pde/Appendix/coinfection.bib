	Issn = {1543-592X},
	Journal = {Annual Review of Ecology Evolution and Systematics},
	Keywords = {biodiversity, conservation, footed ferret recovery, group-size, host behavior, immunodeficiency virus, infectious disease, infectious-diseases, mating system, mating systems, population-dynamics, reproductive success, sexually-transmitted-diseases, social structure, species richness, transmission dynamics, transmission mode},
	Language = {English},
	Note = {WOS:000220102000019},
