	Issn = {0021-8901},
	Journal = {Journal of Applied Ecology},
	Keywords = {age, Bayesian, Bison, bovine brucellosis, Brucella abortus, brucellosis, calves, conservation, culture, disease, epidemiology, livestock, national-park, persistence, predation, prevalence, progeny, serology, wildlife, Yellowstone},
	Language = {English},
