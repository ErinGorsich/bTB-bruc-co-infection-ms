	Doi = {10.1186/1746-6148-10-140},
	File = {How%20commercial%20and%20non-commerc:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/IQH32QEZ/How commercial and non-commerc.pdf:application/pdf},
