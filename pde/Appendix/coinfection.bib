	Issn = {1090-0233},
	Journal = {Veterinary Journal},
	Keywords = {2-component system bvrr/bvrs, abortus rough mutants, Brucella abortus, brucellosis, cattle, culture-positive cows, dendritic cell maturation, immune-responses, intracellular bacterial-infection, iv secretion system, nitric-oxide, nonprofessional phagocytes, Pathogenesis, toll-like receptor-4},
	Language = {English},
	Month = may,
	Note = {WOS:000276878200007},
	Number = {2},
	Pages = {146--155},
	Title = {Pathogenesis of bovine brucellosis},
	Volume = {184},
	Year = {2010},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.tvjl.2009.04.010}}

@article{alexander_buffalo_2012,
	Abstract = {Background: Brucellosis is a zoonotic disease of global importance infecting humans, domestic animals, and wildlife. Little is known about the epidemiology and persistence of brucellosis in wildlife in Southern Africa, particularly in Botswana.   Methods: Archived wildlife samples from Botswana (1995-2000) were screened with the Rose Bengal Test (RBT) and fluorescence polarization assay (FPA) and included the African buffalo (247), bushbuck (1), eland (5), elephant (25), gemsbok (1), giraffe (9), hartebeest (12), impala (171), kudu (27), red lechwe (10), reedbuck (1), rhino (2), springbok (5), steenbok (2), warthog (24), waterbuck (1), wildebeest (33), honey badger (1), lion (43), and zebra (21). Human case data were extracted from government annual health reports (1974-2006).   Findings: Only buffalo (6\%, 95\% CI 3.04\%-8.96\%) and giraffe (11\%, 95\% CI 0-38.43\%) were confirmed seropositive on both tests. Seropositive buffalo were widely distributed across the buffalo range where cattle density was low. Human infections were reported in low numbers with most infections (46\%) occurring in children ({\textless}14 years old) and no cases were reported among people working in the agricultural sector.   Conclusions: Low seroprevalence of brucellosis in Botswana buffalo in a previous study in 1974 and again in this survey suggests an endemic status of the disease in this species. Buffalo, a preferred source of bush meat, is utilized both legally and illegally in Botswana. Household meat processing practices can provide widespread pathogen exposure risk to family members and the community, identifying an important source of zoonotic pathogen transmission potential. Although brucellosis may be controlled in livestock populations, public health officials need to be alert to the possibility of human infections arising from the use of bush meat. This study illustrates the need for a unified approach in infectious disease research that includes consideration of both domestic and wildlife sources of infection in determining public health risks from zoonotic disease invasions.},
	Author = {Alexander, Kathleen Anne and Blackburn, Jason Kenna and Vandewalle, Mark Eric and Pesapane, Risa and Baipoledi, Eddie Kekgonne and Elzer, Phil H.},
	Doi = {10.1371/journal.pone.0032842},
	File = {Alexander et al (2012) brucellosis_zoonoticthreat.pdf:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/F2S6KK8H/Alexander et al (2012) brucellosis_zoonoticthreat.pdf:application/pdf},
	Issn = {1932-6203},
	Journal = {Plos One},
	Keywords = {diagnosis, dynamics, fluorescence-polarization assay, infectious-diseases, national-park, serological survey, sub-saharan africa, syncerus-caffer, tuberculosis, wood bison},
	Language = {English},
	Month = mar,
	Note = {WOS:000303062000017},
	Number = {3},
	Title = {Buffalo, {Bush} {Meat}, and the {Zoonotic} {Threat} of {Brucellosis} in {Botswana}},
	Volume = {7},
	Year = {2012},
	Bdsk-Url-1 = {http://dx.doi.org/10.1371/journal.pone.0032842}}

