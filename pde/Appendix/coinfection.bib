	Issn = {0378-1135},
	Journal = {Veterinary Microbiology},
	Keywords = {Brucella, brucellosis, dna amplification, DNA-typing, genus brucella, insertion-sequence, marine mammals, molecular characterization, molecular diagnostics, nucleotide-sequence, pcr, pcr-rflp, polymerase-chain-reaction, reaction assay, target sequence, vaccine strains},
