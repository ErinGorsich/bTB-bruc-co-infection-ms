	File = {Estimating%20herd%20prevalence%20of:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/GHEMDX95/Estimating herd prevalence of.pdf:application/pdf},
	Issn = {0167-5877},
