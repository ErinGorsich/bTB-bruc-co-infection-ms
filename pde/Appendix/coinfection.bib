@article{mordecai_optimal_2013,
	Abstract = {The ecology of mosquito vectors and malaria parasites affect the incidence, seasonal transmission and geographical range of malaria. Most malaria models to date assume constant or linear responses of mosquito and parasite life-history traits to temperature, predicting optimal transmission at 31 $\,^{\circ}$C. These models are at odds with field observations of transmission dating back nearly a century. We build a model with more realistic ecological assumptions about the thermal physiology of insects. Our model, which includes empirically derived nonlinear thermal responses, predicts optimal malaria transmission at 25 $\,^{\circ}$C (6 $\,^{\circ}$C lower than previous models). Moreover, the model predicts that transmission decreases dramatically at temperatures {\textgreater} 28 $\,^{\circ}$C, altering predictions about how climate change will affect malaria. A large data set on malaria transmission risk in Africa validates both the 25 $\,^{\circ}$C optimum and the decline above 28 $\,^{\circ}$C. Using these more accurate nonlinear thermal-response models will aid in understanding the effects of current and future temperature regimes on disease transmission},
	Annote = {The following values have no corresponding Zotero field:number: 1pub-location: Englandissn: 1461-0248coden: doi: 10.1111/ele.12015citation-id: Mordecai 2013accession-num: 23050931modified-date: 2017-02-20 18:16:12 -0800},
	Author = {Mordecai, Erin A. and Paaijmans, Krijn P. and Johnson, Leah R. and Balzer, Christian and Ben-Horin, Tal and de Moor, Emily and McNally, Amy and Pawar, Samraat and Ryan, Sadie J. and Smith, Thomas C. and Lafferty, Kevin D.},
	Doi = {10.1111/ele.12015},
	File = {Optimal%20temperature%20for%20malari:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/RIGMCADP/Optimal temperature for malari.pdf:application/pdf},
	Issn = {1461-0248},
	Journal = {Ecol Lett},
	Keywords = {Animals, Aquatic Examples, climate change, Culicidae, DDE, Disease \& Mozy Models, Female, host-parasite interactions, humans, malaria, models, Models, Biological, Plasmodium falciparum, research support, non-u.s. gov't, research support, u.s. gov't, non-p.h.s., Temperature},
