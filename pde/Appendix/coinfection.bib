	Issn = {0021-8790},
	Journal = {Journal of Animal Ecology},
	Keywords = {disease ecology, eco-immunology, environment, hamsters phodopus-sungorus, Immunity, lymphocyte-proliferation assays, male tree lizards, mice peromyscus-maniculatus, natural-killer-cells, physiological trade-offs, reproductive effort, siberian hamsters, sickness behavior, stress-induced enhancement},
	Language = {English},
