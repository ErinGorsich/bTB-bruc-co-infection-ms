	Keywords = {bovine spongiform encephalopathy, cattle, CREUTZFELDT-JAKOB-DISEASE, diagnosis, epidemiology, mandatory reporting, RELIABILITY, scrapie, screening, SIGNS, surveillance, SWITZERLAND},
