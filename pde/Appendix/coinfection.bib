@article{garlick_homogenization_2014,
	Abstract = {Chronic wasting disease (CWD) is an infectious prion disease that affects mule deer, along with other Cervids. It is a slow-developing, fatal disease which is rare in the free-ranging deer population of Utah. We present a sex-structured, spatial model for the spread of CWD over heterogeneous landscapes, incorporating both horizontal and environmental transmission pathways. To connect the local movement of deer to the regional spread of CWD, we use ecological diffusion with motility coefficients estimated from mule deer movement data. Ecological diffusion allows for aggregation of populations in desirable habitats and therefore allows for an interaction between density dependent disease transmission and landscape structure. The major innovation presented is use of homogenization to accelerate simulations of disease spread in southeastern Utah, from the La Sal Mountains near Moab to the Abajo Mountains near Monticello. The homogenized model provides accuracy while maintaining fidelity to small-scale habitat effects on deer distribution, including differential aggregation in land cover types with high residence times, with errors comparable to the order parameter measuring separation of small and large scales ( in this case). We use the averaged coefficients from the homogenized model to explore asymptotic invasion speed and the impact of current population size on disease spread in southeastern Utah.},
	Author = {Garlick, Martha J. and Powell, James A. and Hooten, Mevin B. and MacFarlane, Leslie R.},
	Doi = {10.1007/s00285-013-0709-z},
	File = {Garlick et al. (2014) cwd spatial model.pdf:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/4PI7EPGJ/Garlick et al. (2014) cwd spatial model.pdf:application/pdf},
	Issn = {0303-6812},
	Journal = {Journal of Mathematical Biology},
	Keywords = {Asymptotic speed of disease spread, diffusion, Diffusion model, dispersal, dynamics, Ecological diffusion, epidemiology, genetics, infectious-diseases, model, Multi-scale modeling, prion disease, spatial heterogeneity, spatial spread, transmission},
	Language = {English},
	Month = aug,
	Note = {WOS:000339428000004},
	Number = {2},
	Pages = {369--399},
	Title = {Homogenization, sex, and differential motility predict spread of chronic wasting disease in mule deer in southern {Utah}},
	Volume = {69},
	Year = {2014},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s00285-013-0709-z}}

