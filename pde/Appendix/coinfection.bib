	Doi = {/10.1016/j.prevetmed.2013.02.015},
	File = {Temporal%20characterisation%20of%20t:/Users/gorsich/Library/Application Support/Zotero/Profiles/agay1y7z.default/zotero/storage/XW2VC85N/Temporal characterisation of t.pdf:application/pdf},
	Issn = {0167-5877},
	Journal = {Preventive Veterinary Medicine},
	Keywords = {Cattle movements, network analysis, Scale-free, Small-world, Temporal characteristics},
	Month = jul,
