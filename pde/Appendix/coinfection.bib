@article{voyles_pathogenesis_2009,
	Abstract = {The pathogen Batrachochytrium dendrobatidis (Bd), which causes the skin disease chytridiomycosis, is one of the few highly virulent fungi in vertebrates and has been implicated in worldwide amphibian declines. However, the mechanism by which Bd causes death has not been determined. We show that Bd infection is associated with pathophysiological changes that lead to mortality in green tree frogs (Litoria caerulea). In diseased individuals, electrolyte transport across the epidermis was inhibited by {\textgreater}50\%, plasma sodium and potassium concentrations were respectively reduced by {\textasciitilde}20\% and {\textasciitilde}50\%, and asystolic cardiac arrest resulted in death. Because the skin is critical in maintaining amphibian homeostasis, disruption to cutaneous function may be the mechanism by which Bd produces morbidity and mortality across a wide range of phylogenetically distant amphibian taxa.},
	Author = {Voyles, Jamie and Young, Sam and Berger, Lee and Campbell, Craig and Voyles, Wyatt F. and Dinudom, Anuwat and Cook, David and Webb, Rebecca and Alford, Ross A. and Skerratt, Lee F. and Speare, Rick},
	Doi = {10.1126/science.1176765},
	Journal = {Science},
	Month = oct,
	Number = {5952},
	Pages = {582--585},
	Title = {Pathogenesis of {Chytridiomycosis}, a {Cause} of {Catastrophic} {Amphibian} {Declines}},
	Url = {http://www.sciencemag.org/content/326/5952/582.abstract},
	Volume = {326},
	Year = {2009},
	Bdsk-Url-1 = {http://www.sciencemag.org/content/326/5952/582.abstract},
