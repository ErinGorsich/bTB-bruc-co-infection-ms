	Journal = {Epidemiology and infection},
	Keywords = {*Sindbis Virus, Adolescent, Adult, Aged, Aged, 80 and over, Age Factors, Alphavirus Infections/*epidemiology/transmission/virology, Animals, Child, Culex/virology, Female, humans, Male, Middle Aged, Retrospective Studies, Seasons, Sex Factors, South Africa/epidemiology, Young Adult},
