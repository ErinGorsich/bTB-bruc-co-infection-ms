	Issn = {0021-8790},
	Journal = {Journal of Animal Ecology},
	Keywords = {abomasal nematodes, body condition, dna evidence, host-parasite interaction, marshallagia-marshalli, Ostertagia, ostertagia-gruehneri, parasite, parasitism, pathophysiology, population-dynamics, rangifer-tarandus-platyrhynchus, ruminants, soay sheep, svalbard reindeer},
