	Journal = {Theoretical Population Biology},
	Keywords = {approximation, clustering, disease spread, gonorrhea, heterogeneous populations, hiv, infectious-disease, models, patterns, random graph, sexual contacts, small-world networks, spatial correlation, spread, transmission networks},
