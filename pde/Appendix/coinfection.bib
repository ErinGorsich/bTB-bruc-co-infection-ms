	Keywords = {Brucella abortus, cytosolic dna, dendritic cell maturation, domain-containing protein, endoplasmic-reticulum, i interferon, innate immunity, intracellular survival, mice lacking, MyD88, nf-kappa-b, rig-i, TLR signaling, toll-like receptors, trif},
	Language = {English},
	Month = jul,
	Note = {WOS:000307620700018},
